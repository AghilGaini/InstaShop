<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>

  <appSettings>
    <add key="IsTokenExpired" value="false" />
    <add key="TokenExpireTime" value="10" />
    <!--InSeconds-->
    <add key="IsActiveLog" value="true" />
    <add key="LogPattern" value="message,time,timeformat:{MM/dd/yyyy H:mm:ss},classname,line,column" />
    <add key="ErrorPath" value="C:\TJCLogFolder\Error" />
    <add key="WarningPath" value="C:\TJCLogFolder\Warning" />
    <add key="DebugPath" value="C:\TJCLogFolder\Debug" />
    <add key="InfoPath" value="C:\TJCLogFolder\Info" />
    <add key="FatalPath" value="C:\TJCLogFolder\Fatal" />
  </appSettings>

</configuration>